addGrpToPeaks <- function(x){
peaks <- map(x, "mergedPeaks") %>%
map(., data.frame) %>%
imap(., ~{
name <- .y
df <- .x
df %>%
dplyr::mutate(grp = name)
}) %>%
map(., GRanges)
return(peaks)
}
